{
  "description": "demo package",
  "name": "react-3d-route-earth",
  "version": "1.1.0",
  "private": false,
  "dependencies": {
    "@react-three/drei": "^7.0.2",
    "@react-three/fiber": "^7.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "babel-plugin-glsl": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "three": "^0.129.0",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "distribute": "set BABEL_ENV=production NODE_ENV=production && babel src --out-dir dist --copy-files --ignore spec.js,test.js --source-maps",
    "build": "babel src --out-dir dist",
    "cus-build": "set NODE_ENV=production && rm -rf dist && mkdir dist && npx babel src/components --out-dir dist --copy-files",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "react-build": "react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "babel": {
    "presets": [
      "react",
      "env",
      "stage-2",
      "@babel/preset-react",
      "@babel/preset-env"
    ]
  },
  "repository": {
    "type": "git",
